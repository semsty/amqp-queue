{
    "name": "semsty/amqp-queue",
    "description": "yii amqp queue with support delayed restart, etc.",
    "keywords": ["yii", "amqp", "queue"],
    "type": "project",
    "authors": [
        {
            "name": "Semen Khristy",
            "email": "khristysp@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": ">=2.0.38",
        "yiisoft/yii2-queue": ">=2.3.0",
        "php-amqplib/php-amqplib": "*",
        "enqueue/amqp-lib": ">=0.8",
        "ext-sockets": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.4.2",
        "yiisoft/yii2-shell": "2.0.4"
    },
    "config": {
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "semsty\\amqp\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": ""
    }
}
